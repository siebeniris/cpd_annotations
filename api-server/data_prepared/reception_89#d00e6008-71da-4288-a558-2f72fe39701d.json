[{"id": 0, "date": "2015-08-12 00:00:00", "uid": "d152fdb7-4c37-4322-8b4c-98b845564446", "lemma": "['member', 'staff', 'keep', 'call', 'miss', 'think', 'joke']", "sentence": "One member of staff kept calling me Miss I am 62 and at first thought he was joking", "cpt": "0", "category": "reception"}, {"id": 1, "date": "2015-10-17 00:00:00", "uid": "b8157411-dc12-4b58-a697-e1e814640900", "lemma": "['go', 'bed', 'night', 'porter', 'want']", "sentence": "We soon went to bed which was obviously what the night porter was wanting us to do", "cpt": "0", "category": "reception"}, {"id": 2, "date": "2016-02-19 00:37:42", "uid": "3ba0d893-ca4c-46b0-8919-09687213c970", "lemma": "['go', 'reception', 'tell', 'heating', 'work', 'room', 'book', 'reallocate', 'room', 'heater', 'room', 'available']", "sentence": "I went back to reception and was told that as the heating was not working and all rooms booked had been reallocated to rooms with heaters and no other rooms were available", "cpt": "0", "category": "reception"}, {"id": 3, "date": "2016-03-02 23:42:33", "uid": "a1f3e0a3-2c1b-488c-9c21-28a3efede366", "lemma": "['need', 'ask', 'room', 'receptionist', 'spot', 'wedding', 'move', 'quiet', 'building']", "sentence": "We did not need to ask to move rooms the receptionist spotted we were not part of the wedding and moved us to a quiet part of the building", "cpt": "0", "category": "reception"}, {"id": 4, "date": "2016-04-07 15:19:56", "uid": "81ac08ce-0949-4a07-85b7-1d3fd948cc9a", "lemma": "['delay', 'service', 'child', 'table', 'irritable']", "sentence": "Due to the delay in the service the children on that table had become quite irritable", "cpt": "0", "category": "reception"}, {"id": 5, "date": "2016-04-25 00:00:00", "uid": "c1806df9-edec-4554-a9b9-6c76c1dff52e", "lemma": "['awful', 'lackadaisical', 'uncaring', 'hopeless', 'corporate', 'faced', 'customer', 'service', 'company']", "sentence": "Truly awful lackadaisical uncaring hopeless corporate faced customer service by this company", "cpt": "0", "category": "reception"}, {"id": 6, "date": "2016-05-18 08:01:11", "uid": "4f8de810-c0c2-4e70-b75b-e183002b5e15", "lemma": "['food', 'unappealing', 'staff', 'bother', 'small', 'room', 'shower', 'ask', 'prefer', 'bath', 'shower']", "sentence": "The food was unappealing the staff were not that bothered and my very small room did not have a shower but neither was I asked if I would prefer a bath or shower", "cpt": "0", "category": "reception"}, {"id": 7, "date": "2016-06-09 00:00:00", "uid": "e009fe9a-c644-4c66-ac55-51dfe2990021", "lemma": "['room', 'hot', 'air', 'conditioning', 'state', 'hotel', 'description', 'control', 'shower', 'difficult', 'operate', 'stiff', 'age', 'non', 'slip', 'service', 'bath', 'inadequate']", "sentence": "The room was very hot and there was no air conditioning as stated in your hotel description and the controls on the shower were difficult to operate as they were stiff due to age and the non slip service in the bottom of the bath was inadequate", "cpt": "0", "category": "reception"}, {"id": 8, "date": "2016-06-15 16:19:17", "uid": "5b9e5564-e86b-4bb0-a7b3-013187429a11", "lemma": "['staff', 'good', 'arrange', 'checkout', 'schedule', 'room', 'cleaning', 'order']", "sentence": "The staff did their best and arranged a 2PM checkout for us by scheduling the room cleaning order", "cpt": "0", "category": "reception"}, {"id": 9, "date": "2016-08-16 08:52:18", "uid": "9782b94d-2976-40cb-b433-f777e5283e30", "lemma": "['get', 'sausage', 'bacon', 'staff', 'try', 'fill', 'tray', 'know', 'wait', 'ask', 'wait']", "sentence": "whilst getting our sausage and bacon the staff were trying to fill up the trays I know this has to be done but they could have waited or asked us to wait", "cpt": "0", "category": "reception"}, {"id": 10, "date": "2016-11-29 00:00:00", "uid": "1fdc32c0-9585-4850-815d-c35eeea957f5", "lemma": "['clean', 'room', 'good', 'service']", "sentence": "Clean rooms and good service", "cpt": "1", "category": "reception"}, {"id": 11, "date": "2017-01-27 00:00:00", "uid": "bba7b231-6d97-4949-b944-2b6e2b4217d1", "lemma": "['good', 'place', 'great', 'staff']", "sentence": "Always a good place with great staff", "cpt": "1", "category": "reception"}, {"id": 12, "date": "2017-12-04 00:00:00", "uid": "1b5d4c79-23de-44b4-a494-14ce00aaa827", "lemma": "['character', 'facility', 'location', 'helpful', 'attentive', 'staff']", "sentence": "Character facilities location and helpful and attentive staff", "cpt": "1", "category": "reception"}, {"id": 13, "date": "2018-03-23 09:21:47", "uid": "bd58c199-51bf-4c58-9d60-f1db57164943", "lemma": "['excellent', 'quality', 'food', 'customer', 'service']", "sentence": "Excellent quality food and customer service", "cpt": "1", "category": "reception"}, {"id": 14, "date": "2018-05-25 00:00:00", "uid": "7dfd78ee-b452-4160-ab38-f80c41f60205", "lemma": "['staff', 'great', 'look']", "sentence": "Staff were great looked after us well", "cpt": "1", "category": "reception"}, {"id": 15, "date": "2018-08-27 23:43:46", "uid": "b4cd7924-95f4-4abd-8063-ac79356f0949", "lemma": "['arrival', 'staff', 'courteous', 'informative']", "sentence": "Right from arrival staff was courteous and informative", "cpt": "1", "category": "reception"}, {"id": 16, "date": "2018-09-18 16:02:21", "uid": "4e0cf2b8-e3ed-4053-932f-d4b8f4488f97", "lemma": "['staff', 'helpful', 'courteous']", "sentence": "The staff were very helpful and courteous", "cpt": "1", "category": "reception"}, {"id": 17, "date": "2019-05-13 08:49:39", "uid": "0926374d-760b-4bb7-b2bd-4f2274e81110", "lemma": "['nice', 'hotel', 'food', 'service']", "sentence": "Very nice hotel food and service", "cpt": "1", "category": "reception"}, {"id": 18, "date": "2019-11-10 19:14:55", "uid": "e6f34a91-492e-473c-baaf-cbef1a3cfed3", "lemma": "['super', 'friendly', 'helpful', 'staff', 'clean', 'room', 'fabulous', 'food']", "sentence": "Super friendly helpful staff clean room fabulous food", "cpt": "1", "category": "reception"}, {"id": 19, "date": "202